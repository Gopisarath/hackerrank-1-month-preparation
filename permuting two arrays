class Result {

    public static String twoArrays(int k, List<Integer> A, List<Integer> B) {
    int count=0;
    int n=A.size();
    Collections.sort(A);
    Collections.sort(B);
    for(int i=0;i<A.size();i++){
            if(A.get(i)+B.get(n-i-1)<k){
                return "NO";
                
            }
            
        }
    
        return "YES";

    }




}
